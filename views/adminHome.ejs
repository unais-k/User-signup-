<section>
    <div class="container">
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Name</th>
                    <th scope="col">Category</th>
                    <th scope="col">Description</th>
                    <th>Image</th>
                </tr>
            </thead>
            <tbody>
                <%for (let i = 0; i < products.length; i++) { %>
                <tr>
                    <th><%=products[i].id%></th>
                    <td><%=products[i].name%></td>
                    <td><%=products[i].category%></td>
                    <td><%=products[i].description%></td>
                    <td><img style="width: 50px; height: 50px" src="/images/<%=products[i].image %>" /></td>
                </tr>
                <% } %>
            </tbody>
            <tbody>
                <%for (let i = 0; i < products1.length; i++) { %>
                <tr>
                    <th><%=products1[i].id%></th>
                    <td><%=products1[i].name%></td>
                    <td><%=products1[i].category%></td>
                    <td><%=products1[i].description%></td>
                    <td><img style="width: 50px; height: 50px" src="/images/<%=products1[i].image %>" /></td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</section>
